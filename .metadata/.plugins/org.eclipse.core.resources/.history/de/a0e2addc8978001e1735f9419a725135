#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define NUM_REGISTER 16

typedef unsigned int PA

typedef struct PCB_NODE {
	PA pcb_data;
	struct PCB_NODE * before;
	struct PCB_NODE * after;
}PCB_NODE;

PCB_NODE * ptr_PCB_Creator;
PCB_NODE * ptr_PCB_Current;
PCB_NODE * ptr_PCB_Head;

extern PCB_NODE * allocate_pcb(PCB data);
extern void add_pcb(PCB data);
